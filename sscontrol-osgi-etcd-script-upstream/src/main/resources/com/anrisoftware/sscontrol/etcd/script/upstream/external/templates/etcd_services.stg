etcdService(parent, vars) ::= <<
# Authors
# https://koji.fedoraproject.org/koji/buildinfo?buildID=820543
[Unit]
Description=Etcd Server
After=network.target
After=network-online.target
Wants=network-online.target

[Service]
Type=notify
WorkingDirectory=<parent.dataDir>
EnvironmentFile=-<parent.configFile>
User=<parent.user>
# set GOMAXPROCS to number of processors
ExecStart=/bin/bash -c "GOMAXPROCS=$(nproc) <parent.binDir>/etcd --name=\"${ETCD_NAME}\" --data-dir=\"${ETCD_DATA_DIR}\" --listen-client-urls=\"${ETCD_LISTEN_CLIENT_URLS}\""
Restart=on-failure
LimitNOFILE=65536

[Install]
WantedBy=multi-user.target

>>
